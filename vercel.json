{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/dist",
    "installCommand": "npm install",
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/index.js"
      },
      {
        "src": "/assets/(.*)",
        "dest": "frontend/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/dist/index.html"
      }
    ]
  }